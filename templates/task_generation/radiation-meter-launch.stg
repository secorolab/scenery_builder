launch(data) ::= <<
<launch>
    $data:node(); separator="\n\n"$
</launch>
>>

node(obj) ::= <<
<node name="$obj.name$" pkg="uvc_meter" type="uvc_meter">
    <param name="x" type="double" value="$obj.x$" />
    <param name="y" type="double" value="$obj.y$" />
</node>
>>
